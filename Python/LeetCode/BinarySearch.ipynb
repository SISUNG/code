    "69.x的平方根"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "x = 4\n",
    "l = 0\n",
    "r = x / 2 + 1\n",
    "while l <= r:\n",
    "    m = (l + r) / 2\n",
    "    res = int(m * m)\n",
    "    if res < x:\n",
    "        l = m + 1\n",
    "    elif res > x:\n",
    "        r = m - 1\n",
    "    else:\n",
    "        print(m)\n"
   ]
